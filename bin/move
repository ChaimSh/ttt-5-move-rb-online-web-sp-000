#!/usr/bin/env ruby

require_relative '../lib/move.rb'

board = [" "," "," "," "," "," "," "," "," "]
# Code your CLI Here
puts "Welcome to Tic Tac Toe!"

display_board(board)

puts  "Where would you like to go?"

input = gets.strip

input_to_index(input)
input.to_i - 1

move(board, index, value = "X")
board[index] = value

display_board
